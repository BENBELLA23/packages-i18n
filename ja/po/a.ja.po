#. leap/apr/apr-devel/summary
#. tumbleweed/apr/apr-devel/summary
msgid "Development files for the Apache Portable Runtime (APR) library"
msgstr "Apache Portable Runtime (APR) ライブラリ向けの開発用ファイル"

#. leap/apr/apr-devel/description
#. tumbleweed/apr/apr-devel/description
msgid ""
"APR is Apache's Portable Runtime Library, designed to be a support library "
"that provides a predictable and consistent interface to underlying platform-"
"specific implementations.\n"
"\n"
"This subpackage contains header files for developing applications that want "
"to make use of APR."
msgstr ""

#. leap/apr-util/apr-util-devel/summary
#. tumbleweed/apr-util/apr-util-devel/summary
msgid "Development files for the Apache Portable Runtime (APR) Utility Library"
msgstr "Apache Portable Runtime (APR)  ユーティリティライブラリ向け開発用ファイル"

