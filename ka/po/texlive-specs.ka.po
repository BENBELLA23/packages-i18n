#. tumbleweed/texlive-specs-y/texlive-unifith-doc/description
msgid "This package includes the documentation for texlive-unifith"
msgstr ""

#. tumbleweed/texlive-specs-y/texlive-uninormalize/summary
msgid "Unicode normalization support"
msgstr ""

#. tumbleweed/texlive-specs-y/texlive-uninormalize/description
msgid "This package provides Unicode normalization (useful for composed characters) for LuaLaTeX."
msgstr ""

#. tumbleweed/texlive-specs-y/texlive-uninormalize-doc/summary
msgid "Documentation for texlive-uninormalize"
msgstr ""

#. tumbleweed/texlive-specs-y/texlive-uninormalize-doc/description
msgid "This package includes the documentation for texlive-uninormalize"
